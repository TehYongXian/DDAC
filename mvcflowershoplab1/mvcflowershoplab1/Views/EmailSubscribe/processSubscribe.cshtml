<!--subscription success page-->
@{
    ViewData["Title"] = "processSubscribe";
}
<center>
    <h1>Congratulation? You subcribed our shop now!</h1>
    <p>Kindly check your email to confirm your subscription</p>
    <p><b>Your confirm subscription id: </b>@ViewBag.confirmationID</p>
    <br />
    <form asp-action="Index" asp-controller="EmailSubscribe">
        <button type="submit" class="btn-dark">Back to Subscription Page</button>
    </form>
</center>


